<% page_title "Method Not Allowed" %>

<%= title do %>
  405 Method Not Allowed
<% end %>

<%= bootstrap_context_wrapper do %>
  <div class="row">
    <div class="col-xs-12 col-md-12">
      <h2>The HTTP method you used is not allowed.</h2>

      <% content_for(:page_to_track, '/405.html') %>

      <script type="text/javascript">
        $(document).ready(function(){
          gtag('event', 'errors', {'event_action': '405', 'event_label': document.location.pathname + document.location.search + "&from=" + document.referrer})
        });
      </script>
    </div>
  </div>
<% end %>
