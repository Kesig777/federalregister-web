<% header_type 'reader-aids' %>
<% page_title "#{@presenter.name} :: #{@presenter.agency_name}" %>
<% description @presenter.description %>

<% title title_bar_class: "reader-aids" do %>
  <%= fr_icon('indent') %>
  <%= @presenter.name %>
<% end %>

<%= bootstrap_context_wrapper do %>
  <div class="fr-index">

      <div class="page-summary">
        <%= render partial: 'indexes/agency_year_header', locals: {
          presenter: @presenter
        } %>
      </div>

        <div class="doc-toc">
          <%= fr_content_box(
            FrBox.build(
              @presenter.fr_content_box_type,
              title: @presenter.name,
              description: @presenter.description
            )
          ) do %>
            <div class="doc doc-document-issue">
              <%= render partial: 'issues/table_of_contents', locals: {
                agencies: @presenter.agency,
                document_partial: @presenter.document_partial,
                display_agency_name: false,
                fr_index: true
              } %>
            </div>
          <% end %>
        </div>

  </div>

<% end %>
