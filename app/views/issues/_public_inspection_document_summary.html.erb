<!-- issue public inspection metadata -->
<div id="pi-metadata-bar" class="row">
  <div class="fr-head-metadata-bar fr-head-metadata-bar-public-inspection">
    <div class="col-xs-3 col-md-3 fr-head-meta-label-holder">
      <div class="fr-head-meta-label">
        <i class="icon-fr2-clipboard"></i>
        <h4><%= link_to "Public Inspection", '' %></h4>
      </div>

      <span class="icon-fr2 icon-fr2-stop-hand"></span>
    </div>

    <div class="col-xs-9 col-md-9 fr-head-meta-counts">
      <div class="row">
        <div class="col-xs-6 col-md-6">
          <%= render partial: 'issues/public_inspection_description' %>
        </div>

        <div class="col-xs-6 col-md-6">
          <ul class="fr-list with-stats">
            <%= render partial: 'issues/public_inspection_filing_types',
              locals: {presenter: presenter} %>
          </ul>
        </div>
      </div>
    </div>

    <div class="clearfix"></div>
  </div>
</div>
