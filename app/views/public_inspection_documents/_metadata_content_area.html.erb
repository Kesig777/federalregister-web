<div id="metadata_content_area" class="metadata-content-area">
  <h1><%= document.title %></h1>

  <div class="metadata_share_bar">

    <p class="metadata">
      An <span class="pi_warning">unpublished</span>
      <%= document.type %>
      by
      <span class="agencies">
        <%= document.agency_names %>
      </span>

      <% if document.publication_date %>
        on <%= document.publication_date %>
      <% end %>
    </p>
  </div>
</div>
